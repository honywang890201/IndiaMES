<Window x:Class="BoxPrint_M.AddPrint"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:uc="clr-namespace:Component.Controls.User.GenerateControl;assembly=Component" 
        Title="Reprinting" SizeToContent="WidthAndHeight"  ResizeMode="NoResize" WindowStartupLocation="CenterScreen"  Loaded="Window_Loaded" ShowInTaskbar="False">
    <Grid Margin="5,10,5,10">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <TextBlock Text="carton SN.：" Grid.Row="1" Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Right"/>
        <uc:TextBox Name="txtBoxSN"   Grid.Row="1" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center" 
                             Width="400" FontSize="40" FontWeight="Bold" KeyDown="txtBoxSN_KeyDown" IsToUpper="True" Height="60"  Margin="0,5,0,5" />
        <Button Click="ok_print" Height="40" Width="40"  Visibility="Collapsed"></Button>
    </Grid>
</Window>
